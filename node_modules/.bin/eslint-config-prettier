#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yannik/js/toplocs/location-plugin-new/node_modules/.pnpm/eslint-config-prettier@10.1.5_eslint@9.30.1_jiti@1.21.7_/node_modules/eslint-config-prettier/bin/node_modules:/Users/yannik/js/toplocs/location-plugin-new/node_modules/.pnpm/eslint-config-prettier@10.1.5_eslint@9.30.1_jiti@1.21.7_/node_modules/eslint-config-prettier/node_modules:/Users/yannik/js/toplocs/location-plugin-new/node_modules/.pnpm/eslint-config-prettier@10.1.5_eslint@9.30.1_jiti@1.21.7_/node_modules:/Users/yannik/js/toplocs/location-plugin-new/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yannik/js/toplocs/location-plugin-new/node_modules/.pnpm/eslint-config-prettier@10.1.5_eslint@9.30.1_jiti@1.21.7_/node_modules/eslint-config-prettier/bin/node_modules:/Users/yannik/js/toplocs/location-plugin-new/node_modules/.pnpm/eslint-config-prettier@10.1.5_eslint@9.30.1_jiti@1.21.7_/node_modules/eslint-config-prettier/node_modules:/Users/yannik/js/toplocs/location-plugin-new/node_modules/.pnpm/eslint-config-prettier@10.1.5_eslint@9.30.1_jiti@1.21.7_/node_modules:/Users/yannik/js/toplocs/location-plugin-new/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/eslint-config-prettier@10.1.5_eslint@9.30.1_jiti@1.21.7_/node_modules/eslint-config-prettier/bin/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/eslint-config-prettier@10.1.5_eslint@9.30.1_jiti@1.21.7_/node_modules/eslint-config-prettier/bin/cli.js" "$@"
fi
